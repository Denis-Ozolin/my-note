      {
        "id": 2,
        "name": "GitHub with Terminal",
        "notes": [
          {
            "id": 1,
            "instruction": "Cloning a repository",
            "npm": "$ git clone repo-link"
          },
          {
            "id": 2,
            "instruction": "Change status",
            "npm": "$ git status"
          },
          {
            "id": 3,
            "instruction": "Change branch",
            "npm": "$ git checkout name-branch"
          },
          {
            "id": 4,
            "instruction": "Create new branch",
            "npm": "$ git checkout -b name-branch"
          },
          {
            "id": 5,
            "instruction": "Delete branch",
            "npm": "$ git branch --delete name-branch"
          },
          {
            "id": 6,
            "instruction": "Download latest changes",
            "npm": "$ git pull"
          },
          {
            "id": 7,
            "instruction": "Add changes",
            "npm": "$ git add ."
          },
          {
            "id": 8,
            "instruction": "Create commit",
            "npm": "$ git commit -m 'name-changes'"
          },
          {
            "id": 9,
            "instruction": "Send commit",
            "npm": "$ git push"
          },
          {
            "id": 10,
            "instruction": "Send commit to selected branch",
            "npm": "$ git push --set-upstream origin branch-name"
          },
          {
            "id": 11,
            "instruction": "Back to previous commit",
            "npm": "$ git checkout ."
          }
        ]
      },
