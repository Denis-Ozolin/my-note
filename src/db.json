        "notes": [
          {
            "instruction": "Install globally for the first time:",
            "npm": "$ npm install netlify-cli -g"
          },
          {
            "instruction": "Account login:",
            "npm": "$ netlify login"
          },
          {
            "instruction": "Change account:",
            "npm": "$ netlify switch"
          },
          {
            "instruction": "At the first deploy this app:",
            "npm": "$ npm run deploy",
            "descr": "+ Create & configure a new site n\team -> default (Enter),n\name -> uniq-app-name.\nIn the live URL field link to live page."
          },
          {
            "instruction": "Open app:",
            "npm": "$ netlify open --site"
          }
        ],
        "scripts": {
