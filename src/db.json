[
  {
    "id": 1,
    "name": "Hostings",
    "items": [
      {
        "id": 1,
        "name": "Netlify",
        "description": [
          "At the first deployment, create project on Netlify:",
          "+ Create & configure a new site (Enter),",
          "team -> default (Enter), name -> uniq-app-name (Enter).",
          "In the live URL field link to live page."
        ],
        "notes": [
          {
            "id": 1,
            "instruction": "Install globally for the first time:",
            "npm": "$ npm install netlify-cli -g"
          },
          {
            "id": 2,
            "instruction": "Account login:",
            "npm": "$ netlify login"
          },
          {
            "id": 3,
            "instruction": "Check account information:",
            "npm": "$ netlify status"
          },
          {
            "id": 4,
            "instruction": "Change account:",
            "npm": "$ netlify switch"
          },
          {
            "id": 5,
            "instruction": "Deploy app to Netlifi hosting:",
            "npm": "$ npm run deploy"
          },
          {
            "id": 6,
            "instruction": "Open app:",
            "npm": "$ netlify open --site"
          }
        ],
        "scripts": [
          {
            "id": 1,
            "path": "package.json -> scripts",
            "codes": [
              "predeploy: npm run build,",
              "deploy: netlify deploy -p"
            ]
          }
        ]
      },
      {
        "id": 2,
        "name": "Heroku",
        "description": [
          "Download and install the Heroku CLI."
        ],
        "notes": [
          {
            "id": 1,
            "instruction": "Account login:",
            "npm": "$ heroku login"
          },
          {
            "id": 2,
            "instruction": "Connect with your Git repository:",
            "npm": "$ heroku git:remote -a heroku-project-name"
          },
          {
            "id": 3,
            "instruction": "Commit your code to the repository and deploy it to Heroku using Git:",
            "npm": "$ git push heroku main"
          }
        ]
      },
      {
        "id": 3,
        "name": "GitHub-Pages",
        "scripts": [
          {
            "id": 1,
            "path": "-> package.json",
            "codes": [
              "hompage: https://account-name.github.io/repo-name"
            ]
          },
          {
            "id": 2,
            "path": "package.json -> scripts",
            "codes": [
              "predeploy: npm run build",
              "deploy: gh-pages -d build"
            ]
          }
        ]
      }
    ]
  },
  {
    "id": 2,
    "name": "Libraries",
    "items": [
      {
        "id": 1,
        "name": "ESLint Husky Prettier",
        "notes": [
          {
            "id": 1,
            "instruction": "Installing dependencies:",
            "npm": "$ npm install --save-dev prettier eslint"
          },
          {
            "id": 2,
            "instruction": "init lint-staged & husky:",
            "npm": "$ npx mrm@2 lint-staged"
          },
          {
            "id": 3,
            "instruction": "Setting up pre-commit hooks.",
            "link": "https://github.com/goitacademy/react-lint-staged-workshop"
          }
        ]
      },
      {
        "id": 2,
        "name": "Redux",
        "notes": [
          {
            "id": 1,
            "instruction": "Install Redux:",
            "npm": "$ npm install redux"
          },
          {
            "id": 2,
            "instruction": "Redux for React:",
            "npm": "$ npm install react-redux"
          },
          {
            "id": 3,
            "instruction": "Redux Toolkit:",
            "npm": "$ npm install @reduxjs/toolkit"
          },
          {
            "id": 4,
            "instruction": "Redux with Local Storage:",
            "npm": "$ npm install redux-persist"
          },
          {
            "id": 5,
            "instruction": "Actions are output to the console:",
            "npm": "$ npm install --save redux-logger"
          }
        ]
      },
      {
        "id": 3,
        "name": "json-sever",
        "notes": [
          {
            "id": 1,
            "instruction": "Install json-server & indicate the path to DataBase:",
            "npm": "$ npx json-server --watch public/db.json"
          },
          {
            "id": 2,
            "instruction": "start json-server",
            "npm": "$ npm run json-server"
          }
        ],
        "scripts": [
          {
            "id": 1,
            "path": "package.json -> scripts",
            "codes": [
              "json-server: npx json-server --watch public/db.json --port=3001"
            ]
          }
        ]
      }
    ]
  },
